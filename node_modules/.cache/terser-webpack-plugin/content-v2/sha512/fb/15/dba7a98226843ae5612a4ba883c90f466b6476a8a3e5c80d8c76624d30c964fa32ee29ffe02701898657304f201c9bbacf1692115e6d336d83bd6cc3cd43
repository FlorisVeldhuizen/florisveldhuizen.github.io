(){p5.resizeCanvas(p5.windowWidth,p5.windowHeight);};/**\n  * For lack of better rotation in p5.js\n  * @param {vector} p5.Vector  \tThis is the vector (coord) that you want to rotate\n  * @param {pivot}  p5.Vector  \tThis is the vector (coord) you want to pivot around\n  * @param {angle}\tnumber  \t\tThis is the angle of the rotation in radians\n  */function pointRotate(vector,pivot,angle){var s=p5.sin(angle);var c=p5.cos(angle);// translate point back to origin:\nvector.sub(pivot);// rotate point\nvar xnew=vector.x*c-vector.y*s;var ynew=vector.x*s+vector.y*c;// translate point back:\nvector.x=xnew+pivot.x;vector.y=ynew+pivot.y;return vector;}};var Walking_Walking=function Walking(){return/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(index_esm[\"a\" /* ReactP5Wrapper */],{sketch:Walking_sketch});};/* harmony default export */ var pages_Walking = (Walking_Walking);\n// CONCATENATED MODULE: ./src/pages/Rain.jsx\nvar Rain_sketch=function sketch(p5){/*\n  Made by: Floris Veldhuizen\n  */var raindrops=[];var gravity=0.1;var terminalvelocity=20;var rainy=180;var mousesize=100;p5.setup=function(){p5.createCanvas(p5.windowWidth,p5.windowHeight);for(var i=0;i<rainy;i++){raindrops.push(new Drop());}};p5.draw=function(){p5.background(0,0,0,100);for(var i=0;i<raindrops.length;i++){raindrops[i].wind();raindrops[i].fall();raindrops[i].show();}};function Drop(){this.x=p5.random(p5.width);this.y=p5.random(-10,-p5.height);this.speed=p5.random(0,2);this.falling=true;this.splash=false;this.splashlocx=0;this.splashlocy=p5.height;this.splashprocess=0;this.windpower=10;this.show=function(){if(this.splash){p5.stroke(255);p5.line(this.splashlocx,this.splashlocy,this.splashlocx-p5.random(4,11),this.splashlocy-p5.random(5,10));p5.line(this.splashlocx,this.splashlocy,this.splashlocx,this.splashlocy-p5.random(10,16));p5.line(this.splashlocx,this.splashlocy,this.splashlocx+p5.random(4,11),this.splashlocy-p5.random(5,10));p5.fill(p5.color(0));p5.noStroke();p5.circle(this.splashlocx,this.splashlocy,this.splashprocess);this.splashprocess++;if(this.splashprocess>16){this.splash=false;this.falling=true;this.splashprocess=0;}}if(this.falling){p5.stroke(255);p5.line(this.x,this.y,this.x+this.windpower,this.y+this.speed);}};this.fall=function(){if(this.falling){this.x=this.x+this.windpower;this.y=this.y+this.speed;this.speed=this.speed+gravity;if(this.y>p5.height){resetRain(this,p5.height);}else if(collisionRain(this.x,this.y)){resetRain(this,this.y);}if(this.x>p5.width){this.x=0;}if(this.x<0){this.x=p5.width;}}};this.wind=function(){this.windpower=-(p5.width/2-p5.mouseX)/100;};}function collisionRain(x,y){if(x>p5.mouseX-mousesize/2&&x<p5.mouseX+mousesize/2&&y>p5.mouseY&&y<p5.mouseY+mousesize){return true;//if hit\n}else{return false;}}function resetRain(that,splashlocy){that.splash=true;that.falling=false;that.splashlocy=splashlocy;that.splashlocx=that.x;that.x=p5.random(p5.width);that.y=-10;if(that.speed>terminalvelocity)that.speed=terminalvelocity;}p5.windowResized=function(){p5.resizeCanvas(p5.windowWidth,p5.windowHeight);raindrops=[];for(var i=0;i<rainy;i++){raindrops.push(new Drop());}};};var Rain_Rain=function Rain(){return/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(index_esm[\"a\" /* ReactP5Wrapper */],{sketch:Rain_sketch});};/* harmony default export */ var pages_Rain = (Rain_Rain);\n// EXTERNAL MODULE: ./node_modules/babel-preset-react-app/node_modules/@babel/runtime/helpers/esm/slicedToArray.js + 3 modules\nvar slicedToArray = __webpack_require__(49);\n\n// EXTERNAL MODULE: ./node_modules/babel-preset-react-app/node_modules/@babel/runtime/helpers/esm/toConsumableArray.js + 3 modules\nvar toConsumableArray = __webpack_require__(20);\n\n// CONCATENATED MODULE: ./src/pages/helper.js\nvar cleanupFavicon=function cleanupFavicon(){return function(){var metaTag=document.querySelector(\"link[rel=\\\"icon\\\"]\");if(metaTag){console.log(metaTag);metaTag.remove();}};};\n// CONCATENATED MODULE: ./src/pages/FunkySnake.jsx\nvar FunkySnake_sketch=function sketch(p5){/*\n